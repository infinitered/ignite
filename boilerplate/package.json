{
  "name": "spacebox",
  "version": "0.0.1",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "patch": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "patch-package",
    "start": "expo start --dev-client",
    "start:ios": "expo start --dev-client --ios",
    "install:ios": "eas build --platform ios --profile preview",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild:clean": "npx expo prebuild --clean",
    "prettier:write": "prettier --write ."
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "i18next",
          "@hey-api/client-fetch",
          "intl-pluralrules",
          "mobx",
          "mobx-react-lite",
          "mobx-state-tree"
        ]
      }
    }
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.2.2",
    "@expo/metro-runtime": "~4.0.0",
    "@hey-api/client-fetch": "^0.10.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "apisauce": "3.0.1",
    "date-fns": "^4.1.0",
    "expo": "~52.0.7",
    "expo-application": "~6.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.2",
    "expo-localization": "~16.0.0",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "i18next": "^23.14.0",
    "intl-pluralrules": "^2.0.1",
    "mobx": "6.10.2",
    "mobx-react-lite": "4.0.5",
    "mobx-state-tree": "5.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-native": "0.76.9",
    "react-native-drawer-layout": "^4.0.1",
    "react-native-keyboard-controller": "^1.12.7",
    "react-native-mmkv": "3.1.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@testing-library/react-native": "^12.5.2",
    "@types/jest": "^29.2.1",
    "@types/react": "~18.3.12",
    "babel-jest": "^29.2.1",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-reactotron": "^0.1.2",
    "jest": "^29.2.1",
    "jest-expo": "52.0.6",
    "patch-package": "^8.0.0",
    "postinstall-prepare": "1.0.1",
    "prettier": "^3.3.3",
    "reactotron-core-client": "^2.9.4",
    "reactotron-mst": "^3.1.7",
    "reactotron-react-js": "^3.3.11",
    "reactotron-react-native": "^5.0.5",
    "reactotron-react-native-mmkv": "^0.2.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
